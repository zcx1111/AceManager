.drawer {
    margin-top: 60px;
    position: fixed;
    right: -768px;
    width: 768px;
    height: 100%;
    background: #FFF;
    z-index: 200;
    top: 0;
    overflow-x: hidden;
}

#mask {
    display: none;
    position: fixed;
    height: 100%;
    width: 100%;
    z-index: 100;
    background: #000;
    opacity: .15;
    filter: alpha(opacity = 15);
    -moz-opacity: .15;
}