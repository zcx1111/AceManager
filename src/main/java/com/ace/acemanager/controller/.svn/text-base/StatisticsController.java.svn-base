package com.ace.acemanager.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class StatisticsController extends BaseController {

	@RequestMapping(value="/statistics.html")
	public String Statistics(){
		return "statistics/statistics";
	}
	
	@RequestMapping(value="/general/accountSetting.html")
	public String accountSetting(){
		return "general/accountSetting";
	} 
	
	@RequestMapping(value="/general/message.html")
	public String general(){
		return "general/message";
	} 
}
